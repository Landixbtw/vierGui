<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.viergui.vierguiwinnt.Controller">
   <children>
      <BorderPane prefHeight="300.0" prefWidth="400.0">
         <bottom>
            <HBox alignment="BOTTOM_RIGHT" BorderPane.alignment="BOTTOM_RIGHT">
               <children>
                  <TextField fx:id="diaryTextInput" HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" onAction="#handleNeuButton" text="Neu" />
               </children>
            </HBox>
         </bottom>
         <center>
            <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS"
                        BorderPane.alignment="CENTER">
               <content>
                  <VBox fx:id="diaryVBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
               </content>
            </ScrollPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>